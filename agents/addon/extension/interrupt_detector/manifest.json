{
  "type": "extension",
  "name": "interrupt_detector",
  "version": "0.1.0",
  "language": "go",
  "dependencies": [
    {
      "type": "system",
      "name": "rte_runtime",
      "version": "0.1.0"
    },
    {
      "type": "system",
      "name": "rte_runtime_go",
      "version": "0.1.0"
    }
  ],
  "api": {
    "data_in": [
      {
        "name": "text_data",
        "property": {
          "text": {
            "type": "string"
          },
          "is_final": {
            "type": "bool"
          }
        }
      }
    ],
    "cmd_out": [
      {
        "name": "flush"
      }
    ]
  }
}