{
    "type": "extension",
    "name": "openai_chatgpt",
    "version": "0.1.0",
    "language": "go",
    "support": [],
    "dependencies": [
        {
            "type": "system",
            "name": "rte_runtime",
            "version": "0.1.0"
        },
        {
            "type": "system",
            "name": "rte_runtime_go",
            "version": "0.1.0"
        }
    ],
    "api": {
        "property": {
            "api_key": {
                "type": "string"
            },
            "frequency_penalty": {
                "type": "float64"
            },
            "presence_penalty": {
                "type": "float64"
            },
            "model": {
                "type": "string"
            },
            "max_tokens": {
                "type": "int64"
            },
            "prompt": {
                "type": "string"
            },
            "greeting": {
                "type": "string"
            },
            "max_memory_length": {
                "type": "int64"
            }
        },
        "data_in": [
            {
                "name": "text_data",
                "property": {
                    "text": {
                        "type": "string"
                    },
                    "is_final": {
                        "type": "bool"
                    }
                }
            }
        ],
        "data_out": [
            {
                "name": "text_data",
                "property": {
                    "text": {
                        "type": "string"
                    },
                    "end_of_segment": {
                        "type": "bool"
                    }
                }
            }
        ],
        "cmd_in": [
            {
                "name": "flush"
            }
        ],
        "cmd_out": [
            {
                "name": "flush"
            }
        ]
    }
}