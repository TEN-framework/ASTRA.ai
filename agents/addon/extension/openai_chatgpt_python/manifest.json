{
  "type": "extension",
  "name": "openai_chatgpt_python",
  "version": "0.1.0",
  "language": "python",
  "dependencies": [
    {
      "type": "system",
      "name": "rte_runtime_python",
      "version": "0.2.0"
    }
  ],
  "api": {
    "property": {
      "api_key": {
        "type": "string"
      },
      "frequency_penalty": {
        "type": "float64"
      },
      "presence_penalty": {
        "type": "float64"
      },
      "temperature": {
        "type": "float64"
      },
      "top_p": {
        "type": "float64"
      },
      "model": {
        "type": "string"
      },
      "max_tokens": {
        "type": "int64"
      },
      "base_url": {
        "type": "string"
      },
      "prompt": {
        "type": "string"
      },
      "greeting": {
        "type": "string"
      },
      "proxy_url": {
        "type": "string"
      },
      "max_memory_length": {
        "type": "int64"
      }
    },
    "data_in": [
      {
        "name": "text_data",
        "property": {
          "text": {
            "type": "string"
          }
        }
      }
    ],
    "data_out": [
      {
        "name": "text_data",
        "property": {
          "text": {
            "type": "string"
          }
        }
      }
    ]
  }
}